{
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "bracketSpacing": false,
    "bracketSameLine": true,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "tabWidth": 4
            }
        },
        {
            "files": "*.tsx",
            "options": {
                "printWidth": 200,
                "bracketSpacing": true
            }
        },
        {
            "files": "*.ts",
            "options": {
                "printWidth": 200,
                "bracketSpacing": true
            }
        }
    ]
}